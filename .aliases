#!/usr/bin/env zsh

alias todo="$EDITOR ~/.todo"

## docker
alias env-docker="eval $(docker-machine env default)"
alias boot-docker="docker-machine create -d virtualbox --virtualbox-memory 4096 default"

# popular directories
alias pl="cd $DEV/playground"
alias h="cd $DEV/projects/huuuge"
alias hg="cd $DEV/projects/huuuge/gamebingo"
alias hs="cd $DEV/projects/huuuge/serverbingo"

# popular programs
alias log-nim="adb logcat -s NIM_APP"
alias g="git"
alias v="vim"

alias np="cd ~/.nimble/pkgs"
alias nb="cd ~/.nimble/bin"

# zprofile
alias vze="vim ~/.zprofile"
alias cze="cat ~/.zprofile"
alias sze="source ~/.zprofile"

alias vzc="vim ~/.zshrc"
alias vvc="vim ~/.vimrc"

# server
alias wsc="docker-compose -f nix-nakama-vs-cockroachdb-ssl.yml down -v"
alias wsp="docker-compose -f nix-nakama-vs-postgres-ssl.yml down -v"

alias ssc="docker-compose -f nix-nakama-vs-cockroachdb-ssl.yml up"
alias ssp="docker-compose -f nix-nakama-vs-postgres-ssl.yml up"

alias ss-nossl="docker-compose -f nix-nakama-vs-cockroachdb.yml up"
alias was="wsp && ssp"

# builds
alias ba="BINGO_SERVERIP=$IP nake droid -d:local -d:debugAnalytics"
alias banossl="BINGO_SERVERIP=$IP nake droid -d:debug -d:local -d:copyBundles -d:nossl"

alias bd="nake -d:debug -d:local"
alias bdw="nake -d:debug -d:local -d:widescreen"
alias bdsb="nake -d:debug -d:local -d:sandbox"

alias benossl="nake emscripten -d:local -d:copyBundles -d:debug -d:nossl"

alias rd="$BINGO_CLIENT_HOME/build/macosx_debug_nimx/Huuuge_Bingo_Story_debug.app/Contents/MacOS/Huuuge_Bingo_Story_debug"
